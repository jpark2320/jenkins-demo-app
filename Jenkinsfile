pipeline {
  agent any

  stages {

    stage("build") {

      steps {
        echo 'building the application...'
        echo 'build test 1'
      }
    }

    stage("test") {

      steps {
        echo 'testing the application...'
      }
    }

    stage("deploy") {

      steps {
        echo 'deploying the application...'
      }
    }
  }
}
